	year = {1994},
	pmid = {8188726},
	keywords = {Bone and Bones, Bone Development, Elasticity, Elastic Tissue, Heart, Humans, Hypertrophy, Hypertrophy, Left Ventricular, Mathematics, Models, Biological, Stress, Mechanical},

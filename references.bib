	author = {Tryfonidis, Konstantinos and Senkus, Elzbieta and Cardoso, Maria J. and Cardoso, Fatima},
	month = mar,
	year = {2015},
	keywords = {BREAST cancer, Chemotherapy, Clinical trials, Combination drug therapy, Targeted therapies},
